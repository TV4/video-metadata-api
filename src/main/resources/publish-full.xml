      <!-- Optional -->
      <materialType>program</materialType>
      <!-- Optional -->
      <distributor>distributor</distributor>
      <!-- mandatory -->
      <contentIdRef>contentIdRef</contentIdRef>
      <!-- mandatory, attribute threeD is Optional (default to false) and will probably never be used!!! -->
      <videoFormat threeD="true">576i</videoFormat>
      <!-- Optional -->
      <audioList>
        <format language="EN">mono</format>
      </audioList>
      <!-- Optional, but available for most data from Metabanken -->
